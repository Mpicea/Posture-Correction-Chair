--스키마 이름 : MonDB

CREATE TABLE `YYYYMM`  
(  
 `day`       INTEGER NOT NULL ,  --월별 일수 PK 
 `total`     INTEGER DEFAULT NULL ,  -- 총 앉은 시간
 `goodtime`  INTEGER DEFAULT NULL ,  -- 잘 앉은 시간
 `badtime`   INTEGER DEFAULT NULL ,  --잘 못 앉은 시간
 `goodscore` INTEGER DEFAULT NULL ,  -- 잘 앉은 시간 점수 = 잘 앉은 시간 / 총 앉은 시간
  
PRIMARY KEY (`day`)  
);  
  
